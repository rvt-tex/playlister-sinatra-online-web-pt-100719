<h1>Create New Song</h1>
<form class="" action="/songs" method="post">
<p><input id="Name" type="text" name="song_name" placeholder="Name"></p>
<h3>Existing Genres</h3>
<% Genre.all.each do |genre| %>
  <input id="<%= genre.name %>" type="checkbox" name="genres[]" value="<%= genre.id %>"> <%= genre.name %>
<% end %>
<p><input type="text" id="Artist Name" name="artist_name" placeholder="Artist Name"></p>
<input type="submit" name="" value="Create">
</form>